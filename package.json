  "typings": "./dist/index.d.ts",
  "author": "OhMyVerse.io",
  "license": "Apache-2.0",
  "dependencies": {
    "rainbowvis.js": "^1.1.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.13.2",
    "decentraland-ecs": "latest",
    "glob": "^7.1.6",
    "prettier": "^1.19.1",
    "rollup": "^2.35.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Golfcraft/time-color.git"
